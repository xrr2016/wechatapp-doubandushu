page{
    background-color: #f4f4f4;
    font-family: Arial, Helvetica, sans-serif;
}
.search-container{
    position: fixed;
    top: 0;
    left: 0;
    height: 42px;
    width: 97%;
    padding: 0 12rpx;
    background-color: #B28FCE;
    color: #000;
    font-size: 14px;
    font-weight: normal;
    z-index: 10;
}
.search-input{
    background-color: #f4f4f4;
    color: black;
    margin-top: 5px;
    padding: 5px 10px;
    height: 22px;
    border-radius: 6px;
}
.notfound{
    display:none;
}



.footer{
    text-align: center;
    padding: 10rpx 0;
}